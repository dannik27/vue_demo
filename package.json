{
  "name": "vue_demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init-demodata": "node back/routes/punchlist/initStore.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "back": "nodemon --ext js --watch back back/server.js",
    "start": "concurrently --kill-others \"npm run back\" \"npm run serve\"",
    "start:electron": "concurrently --kill-others \"npm run back\" \"npm run electron:serve\"",
    "build-express": "vue-cli-service build --dest ./back/public",
    "build-www": "vue-cli-service build --dest ./www",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/vue-fontawesome": "^0.1.4",
    "@vue/eslint-config-prettier": "^4.0.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.2.1",
    "chart.js": "^2.7.3",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "firebase": "^5.9.1",
    "fuse-js-latest": "^3.1.0",
    "fuse.js": "^3.3.0",
    "hooper": "0.0.6",
    "i": "^0.3.6",
    "jquery": "^3.3.1",
    "luxon": "^1.11.4",
    "morgan": "^1.9.1",
    "nedb": "^1.8.0",
    "npm": "^6.5.0",
    "pg-promise": "^8.5.4",
    "popper.js": "^1.14.7",
    "request-promise": "^4.2.2",
    "sqlite3": "^4.0.6",
    "uuid": "^3.3.2",
    "vue": "^2.5.17",
    "vue-chartjs": "^3.4.0",
    "vue-cookie": "^1.1.4",
    "vue-datetime": "^1.0.0-beta.10",
    "vue-dynamic-star-rating": "^1.0.7",
    "vue-image-lightbox": "^6.1.2",
    "vue-lazyload": "^1.2.6",
    "vue-loading-overlay": "^3.1.1",
    "vue-router": "^3.0.2",
    "vue-smooth-dnd": "^0.2.8",
    "vue-star-rating": "^1.6.1",
    "vue-yandex-maps": "^0.7.15",
    "vuefire": "^2.0.0-alpha.21",
    "vuex": "^3.0.1",
    "weekstart": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-service": "^3.3.1",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "electron": "^11.5.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "node-sass": "^4.11.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "vue-cli-plugin-electron-builder": "^1.0.0-rc.10",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "main": "background.js"
}
